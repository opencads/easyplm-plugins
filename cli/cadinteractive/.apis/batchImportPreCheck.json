{
    "Parameters": {
        "loginInfoPath": "$locate(`appdata/${Environment.UserName}/loginInfo.json`)",
        "loginInfo": "$Json.Load(loginInfoPath)",
        "urlPrefix": "$loginInfo.sys_site",
        "data": ""
    },
    "Headers": {
        "Authorization": "$loginInfo.data"
    },
    "Method": "POST",
    "Url": "$`${urlPrefix}/epm/codesign/rest/epm/batchImportPreCheck`",
    "Body": "$data"
}